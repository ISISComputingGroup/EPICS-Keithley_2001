record(aSub, "$(P)PARSE:VALUE"){
    field(DESC, "Parses the buffer for the values.")
    
    field(SNAM, "parse_readings_for_value")

    field(INPA, "$(P)READINGS")
    field(FTA, "STRING")
    field(NOA, "20")

    field(OUTA, "$(P)CHAN:01:READ:RAW.A PP")
    field(FTVA, "DOUBLE")
    field(NOVA, "1")
    
    field(OUTB, "$(P)CHAN:02:READ:RAW.A PP")
    field(FTVB, "DOUBLE")    
    field(NOVB, "1")
    
    field(OUTC, "$(P)CHAN:03:READ:RAW.A PP")
    field(FTVC, "DOUBLE")
    field(NOVC, "1")
    
    field(OUTD, "$(P)CHAN:04:READ:RAW.A PP")
    field(FTVD, "DOUBLE")
    field(NOVD, "1")
    
    field(OUTE, "$(P)CHAN:05:READ:RAW.A PP")
    field(FTVE, "DOUBLE")
    field(NOVE, "1")
    
    field(OUTF, "$(P)CHAN:06:READ:RAW.A PP")
    field(FTVF, "DOUBLE")
    field(NOVF, "1")
    
    field(OUTG, "$(P)CHAN:07:READ:RAW.A PP")
    field(FTVG, "DOUBLE")   
    field(NOVG, "1")    
    
    field(OUTH, "$(P)CHAN:08:READ:RAW.A PP")
    field(FTVH, "DOUBLE")
    field(NOVH, "1")   
    
    field(OUTI, "$(P)CHAN:09:READ:RAW.A PP")
    field(FTVI, "DOUBLE")    
    field(NOVI, "1")   
    
    field(OUTJ, "$(P)CHAN:10:READ:RAW.A PP")
    field(FTVJ, "DOUBLE")
    field(NOVJ, "1")  
}

