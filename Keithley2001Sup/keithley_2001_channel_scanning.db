record(mbbi, "$(P)READ:MODE"){
    field(DESC, "Sets channels to scan")
    field(DTYP, "Soft Channel")
    field(SCAN, "Passive")
    
    field(ZRVL, "0")
    field(ZRST, "IDLE")
    field(ONVL, "1")
    field(ONST, "SINGLE")
    field(TWVL, "2")
    field(TWST, "MULTI")
}

record(stringin, "$(P)READ:CHANNELS") {
    field(DESC, "String of channels to set to scan")
    field(DTYP, "Soft Channel")
    field(SCAN, "Passive")
}

record(calc, "$(P)READ:_NUM_OF_CHANS") {
    field(INPA, "$(P)CHAN:01:ACTIVE")
    field(INPB, "$(P)CHAN:02:ACTIVE")
    field(INPC, "$(P)CHAN:03:ACTIVE")
    field(INPD, "$(P)CHAN:04:ACTIVE")
    field(INPE, "$(P)CHAN:06:ACTIVE")
    field(INPF, "$(P)CHAN:07:ACTIVE")
    field(INPG, "$(P)CHAN:08:ACTIVE")
    field(INPH, "$(P)CHAN:09:ACTIVE")
    field(CALC, "A+B+C+D+E+F+G+H")
}
